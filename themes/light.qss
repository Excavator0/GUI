QWidget {
}
QPushButton {
  background-color: #326da8;
  color: #fff;
  font-weight: 600;
  border-radius: 8px;
  border: 1px solid #25507a;
  padding: 5px 15px;
  outline: 0px;
}
QPushButton:hover,
QPushButton:focus {
  background-color: #295a8c;
  border: 2px solid #25507a;
}
QPushButton:disabled {
  background-color: gray;
}